# Production Environment Configuration
# Sequential Prompt Chaining - Senior MHealth

# Import base chaining configuration
source .env.chaining

# API Keys Configuration
# XAI API Configuration (1순위)
XAI_API_KEY=your_xai_api_key_here

# OpenAI API Configuration (2순위)
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Configuration (3순위)
GEMINI_API_KEY=your_gemini_api_key_here

# Use XAI as primary model
PRIMARY_LLM=xai
FALLBACK_LLM=openai
TERTIARY_LLM=gemini

# Production settings - start with gradual rollout
ENABLE_CHAINING=true
CHAINING_MODE=gradual  # Start with A/B testing
CHAINING_PERCENTAGE=0.1  # Start with 10% of traffic

# Production logging
LOG_LEVEL=WARNING
ENABLE_CHAIN_TRACE=false  # Disable trace for performance

# Production crisis alerts
ENABLE_CRISIS_ALERTS=true
CRISIS_ALERT_WEBHOOK=https://emergency.hospital.com/crisis
CRISIS_ALERT_EMAIL=emergency@hospital.com
CRISIS_ALERT_SMS=+82-10-1234-5678

# Production metrics
ENABLE_BIGQUERY_METRICS=true
BIGQUERY_PROJECT=senior-mhealth-471815
BIGQUERY_DATASET=ai_metrics
BIGQUERY_TABLE=chain_executions

# Production thresholds
AB_TEST_MIN_SAMPLES=1000
CRISIS_THRESHOLD_CRITICAL=0.85
CRISIS_THRESHOLD_HIGH=0.65
CRISIS_THRESHOLD_MEDIUM=0.45

# Performance alerts
PERF_ALERT_LATENCY_MS=5000
PERF_ALERT_ERROR_RATE=0.01

# Production resource limits
MAX_CONCURRENT_CHAINS=20
CHAIN_MEMORY_LIMIT_MB=1024
CHAIN_MAX_EXECUTION_TIME=180

# Production cache settings
CACHE_TTL=7200  # 2 hours
CACHE_MAX_SIZE=5000

# Production timeouts (optimized)
CRISIS_TIMEOUT=3.0  # Fast crisis detection
VOICE_ANALYSIS_TIMEOUT=25.0
TEXT_ANALYSIS_TIMEOUT=15.0
SINCNET_TIMEOUT=12.0