# Development Environment Configuration
# Sequential Prompt Chaining - Senior MHealth

# Import base chaining configuration
source .env.chaining

# Override for development
ENABLE_CHAINING=true
CHAINING_MODE=full  # Full mode for testing in dev
CHAINING_PERCENTAGE=1.0

# Development-specific settings
LOG_LEVEL=DEBUG
ENABLE_CHAIN_TRACE=true

# API Keys Configuration
# XAI API Configuration (1순위)
XAI_API_KEY=your_xai_api_key_here

# OpenAI API Configuration (2순위)
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Configuration (3순위)
GEMINI_API_KEY=your_gemini_api_key_here

# Use XAI as primary model
CRISIS_MODEL=xai
PRIMARY_LLM=xai
FALLBACK_LLM=openai

# Reduced timeouts for development
CRISIS_TIMEOUT=10.0
VOICE_ANALYSIS_TIMEOUT=60.0
TEXT_ANALYSIS_TIMEOUT=40.0
SINCNET_TIMEOUT=30.0

# Disable external services in development
ENABLE_CRISIS_ALERTS=false
ENABLE_BIGQUERY_METRICS=false

# Lower thresholds for testing
AB_TEST_MIN_SAMPLES=10
CRISIS_THRESHOLD_CRITICAL=0.7
CRISIS_THRESHOLD_HIGH=0.5
CRISIS_THRESHOLD_MEDIUM=0.3

# Development resource limits
MAX_CONCURRENT_CHAINS=5
CHAIN_MEMORY_LIMIT_MB=256